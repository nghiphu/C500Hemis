                </tr>
                <tr>
                    <th>@Html.DisplayNameFor(model => model.IdChuongTrinhDaoTaoNavigation)</th>
                    <td>@Html.DisplayFor(model => model.IdChuongTrinhDaoTaoNavigation.IdChuongTrinhDaoTao)</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>

<div class="text-center mt-4">
    <a asp-action="Edit" asp-route-id="@Model?.IdGiaHanChuongTrinhDaoTao" class="btn btn-primary">Chỉnh sửa</a>
    <a asp-action="Index" class="btn btn-secondary">Quay Lại</a>
