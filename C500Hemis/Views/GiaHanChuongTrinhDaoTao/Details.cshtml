@model C500Hemis.Models.TbGiaHanChuongTrinhDaoTao

@{
    ViewData["Title"] = "Details";
}

<h1 class="text-center text-primary">Chi Tiết Thông Tin</h1>

<div class="container mt-4">
    <div class="table-responsive">
        <table class="table table-bordered">
            <tbody>
                <tr>
                    <th>@Html.DisplayNameFor(model => model.SoQuyetDinhGiaHan)</th>
                    <td>@Html.DisplayFor(model => model.SoQuyetDinhGiaHan)</td>
                </tr>
                <tr>
                    <th>@Html.DisplayNameFor(model => model.NgayBanHanhVanBanGiaHan)</th>
                    <td>@Html.DisplayFor(model => model.NgayBanHanhVanBanGiaHan)</td>
                </tr>
                <tr>
                    <th>@Html.DisplayNameFor(model => model.GiaHanLanThu)</th>
