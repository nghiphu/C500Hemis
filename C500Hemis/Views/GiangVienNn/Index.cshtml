@model IEnumerable<C500Hemis.Models.TbGiangVienNn>

@{
    ViewData["Title"] = "Giảng viên";
}

<h1>Danh sách giảng viên</h1>

<p>
    <a asp-action="Create">Thêm mới giảng viên</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CoQuanChuQuanOnuocNgoai)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCanBoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdNoiDungHoatDongTaiVietNamNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CoQuanChuQuanOnuocNgoai)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCanBoNavigation.IdCanBo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdNoiDungHoatDongTaiVietNamNavigation.IdNoiDungHoatDongTaiVietNam)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdGvnn">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdGvnn">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdGvnn">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
