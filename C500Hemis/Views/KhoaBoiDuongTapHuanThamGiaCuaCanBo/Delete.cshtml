@model C500Hemis.Models.TbKhoaBoiDuongTapHuanThamGiaCuaCanBo

@{
    ViewData["Title"] = "Delete";
}
<style>
            body {
                background-color: antiquewhite;
            }
            tble {
        background-image: url('https://th.bing.com/th/id/OIP.xyVYZ4HavftAL0HCfqz5_QHaEo?w=305&h=191&c=7&r=0&o=5&dpr=1.3&pid=1.7');
        background-repeat: no-repeat;
        background-attachment: inherit;
        background-size: contain;
    }

    dt {
        border: 5px double black;
    }

    dd {
        border: 1px none ;
    }
    .btn-blue {
        background-color: blue;
    }
</style>
<body>
    <h1 align="center"> <strong>XÓA</strong></h1>

<h3>Bạn có chắc chắn muốn xóa mục này không???</h3>
<div>
    <h4>Khoa bồi dưỡng tập huấn tham gia của cán bộ</h4>
    <hr />
       
        <dl class="row" >
            <tble>
        <dt class = "col-sm-2" align="center">
            Khoa bồi dưỡng tập huấn
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TenKhoaBoiDuongTapHuan)
        </dd>
        <dt class = "col-sm-2">
            Đơn vị tổ chức
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DonViToChuc)
        </dd>
        <dt class = "col-sm-2">
           Địa điểm tổ chức
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DiaDiemToChuc)
        </dd>
        <dt class = "col-sm-2">
           Thời gian bắt đầu
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ThoiGianBatDau)
        </dd>
        <dt class = "col-sm-2">
            Thời gian kết thúc
                </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ThoiGianKetThuc)
                </dd>
        <dt class = "col-sm-2">
            Chứng chỉ
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ChungChi)
        </dd>
        <dt class = "col-sm-2">
          Ngày cấp
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NgayCap)
        </dd>
        <dt class = "col-sm-2">
            Cán bộ giảng dạy
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdCanBoNavigation.IdCanBo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdLoaiBoiDuongNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdLoaiBoiDuongNavigation.IdLoaiBoiDuong)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdNguonKinhPhiNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdNguonKinhPhiNavigation.IdNguonKinhPhi)
                </dd>
        </tble>
        </dl>
       
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdKhoaBoiDuongTapHuanThamGiaCuaCanBo" />
        <input type="submit" value="Xóa" class="btn btn-danger" /> |
        <a asp-action="Index">Quay lại</a>
    </form>
</div>
