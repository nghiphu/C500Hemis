@model C500Hemis.Models.TbQuaTrinhCongTacCuaCanBo

@{
    ViewData["Title"] = "Xóa";
}

<h1 class="text-center">Xóa</h1>

<h3 class="text-center">Bạn có chắc chắn muốn xóa không?</h3>
<div class="container">
    <h4>Thông tin chi tiết</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Từ Tháng/Năm
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TuThangNam)
        </dd>
        <dt class="col-sm-2">
            Đến Tháng/Năm
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DenThangNam)
        </dd>
        <dt class="col-sm-2">
            Đơn vị công tác
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DonViCongTac)
        </dd>
        <dt class="col-sm-2">
            Id Cán bộ
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdCanBoNavigation.IdCanBo)
        </dd>
        <dt class="col-sm-2">
            Chức danh giảng viên
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdChucDanhGiangVienNavigation.IdChucDanhGiangVien)
        </dd>
        <dt class="col-sm-2">
            Chức vụ
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdChucVuNavigation.IdChucVu)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdQuaTrinhCongTacCuaCanBo" />
        <div class="text-center">
            <input type="submit" value="Xóa" class="btn btn-danger" />
            <a class="btn btn-secondary" asp-action="Index">Quay lại danh sách</a>
        </div>
    </form>
</div>
