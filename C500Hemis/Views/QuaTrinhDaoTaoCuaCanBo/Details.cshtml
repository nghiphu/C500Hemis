@model C500Hemis.Models.TbQuaTrinhDaoTaoCuaCanBo

@{
    ViewData["Title"] = "Chi Tiết Quá Trình Đào Tạo";
}

<h1 class="text-center">Chi Tiết Quá Trình Đào Tạo</h1>

<div class="container">
    <h4>Chi tiết </h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-3">
            Cơ sở đào tạo
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.CoSoDaoTao)
        </dd>
        <dt class="col-sm-3">
            Thời gian bắt đầu
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.ThoiGianBatDau)
        </dd>
        <dt class="col-sm-3">
            Thời gian kết thúc
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.ThoiGianKetThuc)
        </dd>
        <dt class="col-sm-3">
            Năm tốt nghiệp
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.NamTotNghiep)
        </dd>
        <dt class="col-sm-3">
            Id Cán bộ
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.IdCanBoNavigation.IdCanBo)
        </dd>
        <dt class="col-sm-3">
            Loại hình đào tạo
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.IdLoaiHinhDaoTaoNavigation.IdLoaiHinhDaoTao)
        </dd>
        <dt class="col-sm-3">
            Nghành đào tạo
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.IdNganhDaoTaoNavigation.IdNganhDaoTao)
        </dd>
        <dt class="col-sm-3">
            Quốc gia đào tạo
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.IdQuocGiaDaoTaoNavigation.IdQuocTich)
        </dd>
        <dt class="col-sm-3">
            Trình độ đào tạo
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.IdTrinhDoDaoTaoNavigation.IdTrinhDoDaoTao)
        </dd>
    </dl>
</div>

<div class="text-center">
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.IdQuaTrinhDaoTaoCuaCanBo">Sửa</a>
    <a class="btn btn-secondary" asp-action="Index">Quay lại danh sách</a>
</div>
