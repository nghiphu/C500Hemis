<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - C500Hemis</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/C500Hemis.styles.css" asp-append-version="true" />



      <link href="https://cdn.datatables.net/v/dt/jq-3.7.0/dt-2.1.7/r-3.0.3/datatables.min.css" rel="stylesheet">
    <script src="https://cdn.datatables.net/v/dt/jq-3.7.0/dt-2.1.7/r-3.0.3/datatables.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.7.1.js"></script>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">C500Hemis</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/Home/CB">Cán bộ (CB) </a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/Home/htqt">Hợp tác quốc tế (HTQT, HTDN) </a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/Home/KHCN">khoa học công nghệ</a>
                        </li

                         <li class="nav-item">
                            <a class="nav-link text-dark" href="/Home/NH">người học </a>
                        </li>

                         <li class="nav-item">
                            <a class="nav-link text-dark" href="/Home/CSVC">cơ sở vật chất </a>
                        </li>

                         <li class="nav-item">
                            <a class="nav-link text-dark" href="/Home/CSGD">cơ sở giáo dục </a>
                        </li>

                         <li class="nav-item">
                            <a class="nav-link text-dark" href="/Home/CTDT">chương trình đào tạo </a>
                        </li>

                         <li class="nav-item">
                            <a class="nav-link text-dark" href="/Home/TCTS">tài chính </a>
                        </li>

                         <li class="nav-item">
                            <a class="nav-link text-dark" href="/Home/TS">tuyển sinh </a>
                       </li>
                          <li class="nav-item">
                            <a class="nav-link text-dark" href="/Home/VB">Văn bằng</a>
                        </li>


                         <li class="nav-item">
                            <a class="nav-link text-dark" href=""></a>
                        </li>

                       
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - C500Hemis - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
