@model C500Hemis.Models.TbDoanCongTac

@{
    ViewData["Title"] = "Delete";
}

<h1 >Xóa</h1>

<h3 > Bạn có chắc chắn muốn xóa?</h3>

<div style="margin:auto">
    <h4 style="text-align:center">Đoàn công tác</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Mã đoàn công tác
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MaDoanCongTac)
        </dd>
        <dt class="col-sm-2">
            Tên đoàn công tác
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TenDoanCongTac)
        </dd>
        <dt class="col-sm-2">
            Số quyết định
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SoQuyetDinh)
        </dd>
        <dt class="col-sm-2">
            Ngày quyết định
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NgayQuyetDinh)
        </dd>
        <dt class="col-sm-2">
            Thời gian bắt đầu
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ThoiGianBatDau)
        </dd>
        <dt class="col-sm-2">
            Thời gian kết thúc
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ThoiGianketThuc)
        </dd>
        <dt class="col-sm-2">
            Mục đích công tác
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MucDichCongTac)
        </dd>
        <dt class="col-sm-2">
            Kết quả công tác
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.KetQuaCongTac)
        </dd>
        <dt class="col-sm-2">
            Phân loại đoàn ra/vào
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdPhanLoaiDoanRaDoanVaoNavigation.IdPhanLoaiDoanRaDoanVao)
        </dd>
        <dt class="col-sm-2">
            Quốc gia đoàn được cử
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdQuocGiaDoanNavigation.IdQuocTich)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdDoanCongTac" />
        <input type="submit" value="Xóa" class="btn btn-danger" /> |
        <a asp-action="Index"> Quay lại</a>
    </form>
</div>
