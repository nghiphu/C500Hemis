@model C500Hemis.Models.TbTrinhDoTiengDanToc

@{
    ViewData["Title"] = "Xóa";
}

<h1 class="text-center">Xóa</h1>

<h3 class="text-center">Bạn có chắc chắn muốn xóa không?</h3>
<div class="container">
    <h4>Thông tin chi tiết</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Id Cán bộ
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdCanBoNavigation.IdCanBo)
        </dd>
        <dt class="col-sm-2">
            Khung năng lực Ngoại ngữ
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdKhungNangLucNgoaiNguNavigation.IdKhungNangLucNgoaiNgu)
        </dd>
        <dt class="col-sm-2">
            Tiếng dân tộc
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdTiengDanTocNavigation.IdTiengDanToc)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdTrinhDoTiengDanToc" />
        <div class="text-center">
            <input type="submit" value="Xóa" class="btn btn-danger" />
            <a class="btn btn-secondary" asp-action="Index">Quay lại danh sách</a>
        </div>
    </form>
</div>
