@model C500Hemis.Models.TbTrinhDoTiengDanToc

@{
    ViewData["Title"] = "Chi Tiết Trình Độ Tiếng Dân Tộc";
}

<h1 class="text-center">Chi Tiết Trình Độ Tiếng Dân Tộc</h1>

<div class="container">
    <h4>Thông tin chi tiết</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Id Cán bộ
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdCanBoNavigation.IdCanBo)
        </dd>
        <dt class="col-sm-2">
            Khung năng lực Ngoại ngữ
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdKhungNangLucNgoaiNguNavigation.IdKhungNangLucNgoaiNgu)
        </dd>
        <dt class="col-sm-2">
            Tiếng dân tộc
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdTiengDanTocNavigation.IdTiengDanToc)
        </dd>
    </dl>
</div>

<div class="text-center">
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.IdTrinhDoTiengDanToc">Sửa</a>
    <a class="btn btn-secondary" asp-action="Index">Quay lại danh sách</a>
</div>
