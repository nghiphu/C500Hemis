@model IEnumerable<C500Hemis.Models.TbTrinhDoTiengDanToc>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<form asp-controller="TrinhDoTiengDanToc" asp-action="Index" method="get">
    <input type="number" name="Search" placeholder="Tìm kiếm theo Id cán bộ" />
    <input type="submit" value="Tìm kiếm" />
</form>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdCanBoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdKhungNangLucNgoaiNguNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTiengDanTocNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdCanBoNavigation.IdCanBo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdKhungNangLucNgoaiNguNavigation.IdKhungNangLucNgoaiNgu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTiengDanTocNavigation.IdTiengDanToc)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTrinhDoTiengDanToc">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTrinhDoTiengDanToc">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTrinhDoTiengDanToc">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
